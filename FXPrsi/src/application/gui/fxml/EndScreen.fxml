         <VBox.margin>
            <Insets left="100.0" />
         </VBox.margin>
      </HBox>
      <Button fx:id="backBtn" mnemonicParsing="false" onMouseClicked="#handleBackBtnClick" prefHeight="127.0" prefWidth="353.0" stylesheets="@../css/menu.css" text="Zpět do menu">
